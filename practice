    <script>
      const chapterSelect = $("#chapter-select");
      for (var i = 0; i <= 143; i++) {
        chapterSelect.append("<option val=\"" + i + "\">" + i + "</option>");
      }

      chapterSelect.on("change", function() {
        $("#start").css('display', 'none');
        $("#card").css('display', 'block');

        window.__kanji_data_index = 0;
        window.__kanji_term = $("#term");

        const chapter = this.value;
        var dataIndex = 0
        var data = window.__kanji_data.filter(function(term) {
        	return "" + term.chapter === chapter;
        });

	    for (let i = data.length - 1; i > 0; i--) {
	        const j = Math.floor(Math.random() * (i + 1));
	        var temp = data[i];
	        data[i] = data[j];
	        data[j] = temp;
	    }

        const term = $("#term");
        const readingInput = $("#readingInput");
        const answerButton = $("#submitAnswerButton");
        const previousAnswer = $("#previousAnswer");
        const kanjiAnswer = $("#kanjiAnswer");
        const readingAnswer = $("#readingAnswer");
        const meaningAnswer = $("#meaningAnswer");
        const progress = $("#progress");

        answerButton.click(function() {
          const input = readingInput.val();
          const cleanFinalん = input.endsWith("n") ?
              input.substring(0, input.length - 1) + "ん" : input;
          const correct = data[dataIndex].reading.split("、").includes(cleanFinalん);
          const currentTerm = data[dataIndex];
          
          // Handle error/correct
          previousAnswer.css("display", "block");
          previousAnswer.css(
          	'background',
          	correct ? "#E2FFB8" : "#FFC5B8");
          if (correct) {
            progress.css("width", Math.round(dataIndex * 100 / data.length).toString() + "%");
            dataIndex++;
          } else {
            const shiftIndex = Math.floor(Math.random() * Math.floor(data.length - dataIndex));
            const insertIndex = dataIndex + shiftIndex;
            data.splice(dataIndex, 1);
            data.splice(
                insertIndex > data.length - 1 ? data.length - 1 : insertIndex,
                0,
                currentTerm);
            console.log(data.splice(dataIndex -1, 3));
          }

          kanjiAnswer.html(currentTerm.kanji);
          readingAnswer.html(
            (correct ? "" : cleanFinalん + " != ") + currentTerm.reading);
          meaningAnswer.html(currentTerm.meaning);

          
          term.html(data[dataIndex].kanji);
          readingInput.val("");
          readingInput.focus();
        });

        term.html(data[dataIndex].kanji);
        readingInput.focus();

        readingInput.keyup(function() {
        	var keycode = (event.keyCode ? event.keyCode : event.which);
        	console.log(keycode);
	        if(keycode == '13'){
                answerButton.trigger("click");
                return;
            }

        	readingInput.val(window.__convert_to_hiragana(readingInput.val()));
        });
      }); 
    </script>

    <script>
	      window.__convert_to_hiragana = function(str) {
	      	console.log(str);
	      	if (str == "") {
              return "";
            }

	        if (str.startsWith("ka")) {
	          return "か" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ki")) {
	          return "き" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ku")) {
	          return "く" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ke")) {
	          return "け" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ko")) {
	          return "こ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ga")) {
	          return "が" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("gi")) {
	          return "ぎ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("gu")) {
	          return "ぐ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ge")) {
	          return "げ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("go")) {
	          return "ご" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("sa")) {
	          return "さ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("shi")) {
	          return "し" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("si")) {
	          return "し" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("su")) {
	          return "す" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("se")) {
	          return "せ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("so")) {
	          return "そ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("za")) {
	          return "ざ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("zi")) {
	          return "じ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("zu")) {
	          return "ず" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ze")) {
	          return "ぜ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("zo")) {
	          return "ぞ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ta")) {
	          return "た" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("chi")) {
	          return "ち" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("ci")) {
	          return "ち" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("tsu")) {
	          return "つ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("tu")) {
	          return "つ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("te")) {
	          return "て" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("to")) {
	          return "と" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("da")) {
	          return "だ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ji")) {
	          return "じ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("du")) {
	          return "づ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("de")) {
	          return "で" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("do")) {
	          return "ど" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
            if (str.startsWith("na")) {
	          return "な" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ni")) {
	          return "に" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("nu")) {
	          return "ぬ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ne")) {
	          return "ね" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }	        
	        if (str.startsWith("no")) {
	          return "の" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ha")) {
	          return "は" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("hi")) {
	          return "ひ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("fu")) {
	          return "ふ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("he")) {
	          return "へ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ho")) {
	          return "ほ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ba")) {
	          return "ば" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("bi")) {
	          return "び" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("bu")) {
	          return "ぶ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("be")) {
	          return "べ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("bo")) {
	          return "ぼ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("pa")) {
	          return "ぱ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("pi")) {
	          return "ぴ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("pu")) {
	          return "ぷ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("pe")) {
	          return "ぺ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("po")) {
	          return "ぽ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ma")) {
	          return "ま" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("mi")) {
	          return "み" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("mu")) {
	          return "む" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("me")) {
	          return "め" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("mo")) {
	          return "も" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ya")) {
	          return "や" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("yu")) {
	          return "ゆ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("yo")) {
	          return "よ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ra")) {
	          return "ら" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ri")) {
	          return "り" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ru")) {
	          return "る" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("re")) {
	          return "れ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("ro")) {
	          return "ろ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("wa")) {
	          return "わ" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("wo")) {
	          return "を" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }
	        if (str.startsWith("kya")) {
	          return "きゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("kyu")) {
	          return "きゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("kyo")) {
	          return "きゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("gya")) {
	          return "ぎゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("gyu")) {
	          return "ぎゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("gyo")) {
	          return "ぎょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("sha")) {
	          return "しゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("shu")) {
	          return "しゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("sho")) {
	          return "しょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("ja")) {
	          return "じゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("ju")) {
	          return "じゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("jo")) {
	          return "じょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("cha")) {
	          return "ちゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("chu")) {
	          return "ちゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("cho")) {
	          return "ちょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("nya")) {
	          return "にゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("nyu")) {
	          return "にゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("nyo")) {
	          return "にょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("mya")) {
	          return "みゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("myu")) {
	          return "みゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("myo")) {
	          return "みょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("hya")) {
	          return "ひゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("hyu")) {
	          return "ひゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("hyo")) {
	          return "ひょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("rya")) {
	          return "りゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("ryu")) {
	          return "りゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("ryo")) {
	          return "りょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("bya")) {
	          return "びゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("byu")) {
	          return "びゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("byo")) {
	          return "びょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("pya")) {
	          return "ぴゃ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("pyu")) {
	          return "ぴゅ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("pyo")) {
	          return "ぴょ" + window.__convert_to_hiragana(str.substring(3, str.length));
	        }
	        if (str.startsWith("nn")) {
	          return "ん" + window.__convert_to_hiragana(str.substring(2, str.length));
	        }

	        if (str.startsWith("kk") || str.startsWith("gg") || str.startsWith("ss") || str.startsWith("zz") || str.startsWith("tt") || str.startsWith("dd") || str.startsWith("hh") || str.startsWith("bb") || str.startsWith("pp") || str.startsWith("mm") || str.startsWith("yy") || str.startsWith("rr") || str.startsWith("ww")) {
	          return "っ" + window.__convert_to_hiragana(str.substring(1, str.length));
	        }

	        if (str.startsWith("a")) {
	          return "あ" + window.__convert_to_hiragana(str.substring(1, str.length));
	        }
            if (str.startsWith("i")) {
	          return "い" + window.__convert_to_hiragana(str.substring(1, str.length));
	        }
	        if (str.startsWith("u")) {
	          return "う" + window.__convert_to_hiragana(str.substring(1, str.length));
	        }
	        if (str.startsWith("e")) {
	          return "え" + window.__convert_to_hiragana(str.substring(1, str.length));
	        }
	        if (str.startsWith("o")) {
	          return "お" + window.__convert_to_hiragana(str.substring(1, str.length));
	        }

            const array = Array.from(str); 
            const mustBeん =
                array[0] === "n" &&
                array.length > 1 &&
                array[1] !== "a" &&
                array[1] !== "e" &&
                array[1] !== "i" &&
                array[1] !== "o" &&
                array[1] !== "u" &&
                array[1] !== "y";
            return (mustBeん ? "ん" : array[0]) +
                window.__convert_to_hiragana(array.slice(1).join(""));
            
	      };
